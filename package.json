{
  "name": "designkit-layout",
  "version": "0.0.1",
  "description": "Sass module for css *.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rightscale-design/designkit-layout.git"
  },
  "author": {
    "name": "Jason Melgoza",
    "email": "jason@rightscale.com"
  },
  "main": "css/designkit-layout.css",
  "style": "css/designkit-layout.css",
  "scss": "src/index.scss",
  "files": [
    "src",
    "css"
  ],
  "keywords": [
    "css",
    "Sass"
  ],
  "license": "MIT",
  "homepage": "https://github.com/rightscale-design/designkit-layout#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.0",
    "bourbon": "^4.2.7",
    "cssmin": "^0.4.3",
    "http-server": "^0.9.0",
    "node-sass": "^3.8.0",
    "nodemon": "^1.10.0",
    "postcss-cli": "^2.5.2"
  },
  "scripts": {
    "build-css": "./bin/build-css",
    "build-min": "./bin/build-min",
    "watch-css": "./bin/watch-css",
    "serve": "./bin/http-server",
    "prefix:css": "postcss -u autoprefixer -b 'last 2 versions' < css/designkit-layout.css | cssmin > dist/designkit-layout.css",
    "prefix:min": "postcss -u autoprefixer -b 'last 2 versions' < css/designkit-layout.css | cssmin > dist/designkit-layout.min.css"
  }
}
